    background: #E0E0E0;         /* Same color as the groove */
    border-radius: 4px;          /* Rounded edges for the sub-page */
}

QSlider::add-page:horizontal {
    background: #E0E0E0;         /* Same color as the groove */
    border-radius: 4px;          /* Rounded edges for the add-page */
